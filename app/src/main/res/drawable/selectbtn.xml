            <solid android:color="#fc6600" />
            <corners android:radius="50dp" />

        </shape>
    </item>

    <item android:state_enabled="false">

    <shape>

        <solid android:color="#BEBEBE" />
        <corners android:radius="50dp" />

    </shape>

</item>

    <item>

        <shape>

            <solid android:color="#950dce" />
            <corners android:radius="50dp" />

        </shape>

    </item>


</selector>